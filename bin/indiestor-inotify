#------------------------------------------------------------
PATH=/bin:/sbin:/usr/bin:/usr/sbin
export PATH
args=''
for i in "$@"
do
    arg=$(echo "$i" | tr ' ' '|') 
    args="$args $arg"
done
eval set -- $args
echo $args
super indiestor-inotify $args

